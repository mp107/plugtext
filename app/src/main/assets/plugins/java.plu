plugapi=1
plugauthors=mp107
plugname=Java
plugdescription=Java Syntax Highlight Plugin
plugversion=1

# comment...
fileextensions-list=java
numbers-regex=\b(\d*[.]?\d+)\b
preprocessors-regex=^[\t ]*(#define|#undef|#if|#ifdef|#ifndef|#else|#elif|#endif|#error|#pragma|#extension|#version|#line)\b
keywords-list=import,package,public,protected,private,abstract,static,final,extends,implements,break,continue,do,for,while,if,else,float,int,void,boolean,true,false,return,struct,foreach,try,throw,new,catch,finally,synchronized,TODO,instnaceof
builtins-list=print,println,this,super,printStackTrace,close,open,show,close,writeBytes,valueOf,toString
comments-regex=/\*(?:.|[\n\r])*?\*/|//.*
